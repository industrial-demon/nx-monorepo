  "license": "MIT",
  "scripts": {
    "_comment-angular": "-=======Angular section========-",
    "add:angular-app": "npx g @nx/angular:app",
    "angular-app:serve": "npx nx serve angular-app",
    "angular-app:build": "npx nx build angular-app",
    "angular:add-component": "npx nx g @nx/angular:component --project=angular-app --module=app.module.ts",
    "angular-app:add-directive": "npx nx g @nx/angular:directive --project=angular-app",
    "angular-app:add-material": "npm i @angular/material && npx nx g @angular/material:ng-add --project=angular-app",
    "_comment-next": "-=======Next========-",
    "add:next-app": "npx nx g @nx/next:app",
    "add:nest-app": "npx g @nx/nest:app",
    "next-app:serve": "npx nx serve next-app",
    "next-app:build": "npx nx build next-app",
    "add:lib": "nx g @nx/react:library ui --unitTestRunner=vitest --bundler=none --directory=weabapp/shared",
    "solid-app:serve": "npx nx serve solid-app",
    "dev": "nx run-many --parallel --target=serve --projects=nest-server,webapp",
    "add:storybook": "npx nx g @nx/react:storybook-configuration",
    "ui-kit:add-component": "npx nx g @nx/react:component --project=ui-kit --export",
    "delete:app": "npx nx g @nx/workspace:remove",
    "add:react-lib": "npx nx g @nx/react:library ui-kit --unitTestRunner=vitest --publishable=true --importPath=@webapp/ui-kit --directory=webapp/modules/shared",
    "webapp:clean:api-connectors": "npx rimraf ./apps/webapp/src/shared/api/generated/connectors",
    "webapp:api-auth": "npx @openapitools/openapi-generator-cli generate -c ./openapi/configs/auth.config.yaml -t ./openapi/templates/mustaches/",
    "webapp:serve": "npx nx serve webapp",
    "webapp:build": "npx nx build webapp",
    "webapp-ui-kit:storybook": "npx nx run webapp-modules-shared-ui-kit:storybook",
    "webapp-ui-kit:build": "npx nx run webapp-modules-shared-ui-kit:build --configuration=development",
    "libs:api-connectors": "npx rimraf ./libs/api/src/connectors && npx @openapitools/openapi-generator-cli generate -c ./openapi/configs/connectors.config.yaml -t ./openapi/templates/mustaches",
    "openapi:template": "npx openapi-generator-cli author template -g typescript-axios -o ./openapi/templates/mustaches",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "npx prisma db seed",
    "prisma:reset": "npx ts-node prisma/db-reset.ts",
    "prisma:migrate": "npx prisma migrate dev --name",
    "prepare": "husky install",
    "lint": "npx nx lint webapp",
    "start": "node dist/apps/nest-server",
    "build": "npx nx run nest-server:build",
    "icons": "npx rimraf ./icons && npx @svgr/cli --jsx-runtime automatic --ref --title-prop --typescript --out-dir ./icons -- ./assets/icons"
  },
